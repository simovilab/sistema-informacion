@prefix gtfs: <http://example.org/gtfs#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gtfs:Stop a rdfs:Class ;
    rdfs:label "Stop" ;
    rdfs:comment "A location where vehicles pick up or drop off passengers." .

gtfs:stop_id a rdf:Property ;
    rdfs:domain gtfs:Stop ;
    rdfs:range xsd:string ;
    rdfs:label "stop_id" ;
    rdfs:comment "Unique identifier for a stop." .

gtfs:stop_name a rdf:Property ;
    rdfs:domain gtfs:Stop ;
    rdfs:range xsd:string ;
    rdfs:label "stop_name" ;
    rdfs:comment "Name of the stop." .

gtfs:stop_lat a rdf:Property ;
    rdfs:domain gtfs:Stop ;
    rdfs:range xsd:decimal ;
    rdfs:label "stop_lat" ;
    rdfs:comment "Latitude of the stop." .

gtfs:stop_lon a rdf:Property ;
    rdfs:domain gtfs:Stop ;
    rdfs:range xsd:decimal ;
    rdfs:label "stop_lon" ;
    rdfs:comment "Longitude of the stop." .

# Example instance
gtfs:stop_123 a gtfs:Stop ;
    gtfs:stop_id "123" ;
    gtfs:stop_name "Main Street Station" ;
    gtfs:stop_lat 40.7128 ;
    gtfs:stop_lon -74.0060 .