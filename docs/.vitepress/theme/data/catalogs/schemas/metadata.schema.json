{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Metadata Schema for Architecture Catalogs",
  "type": "object",
  "required": [
    "title",
    "version",
    "date",
    "description",
    "author",
    "organization",
    "contact",
    "license"
  ],
  "properties": {
    "title": { "type": "string" },
    "version": { "type": "string" },
    "date": { "type": "string", "format": "date" },
    "description": { "type": "string" },
    "author": { "type": "array", "items": { "type": "string" } },
    "organization": { "type": "string" },
    "contact": { "type": "string" },
    "license": { "type": "string" }
  },
  "additionalProperties": false
}
